{
  "name": "plangular",
  "version": "3.0.0-beta-1",
  "description": "Angular directive for custom SoundCloud players",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "browserify": "browserify src/plangular.js --standalone=plangular -o dist/plangular.js",
    "watchify": "watchify src/plangular.js --standalone=plangular -o dist/plangular.js",
    "docs": "node docs/build",
    "watch:docs": "watch 'npm run docs' ./docs",
    "serve": "http-server -p 8000",
    "start": "npm run watchify & npm run watch:docs & npm run serve"
  },
  "dependencies": {
    "audio-player": "^1.0.0",
    "hhmmss": "^1.0.0",
    "soundcloud-resolve-jsonp": "^1.0.0"
  },
  "devDependencies": {
    "blkmark": "^1.0.1",
    "browserify": "^9.0.3",
    "http-server": "^0.7.5",
    "lodash": "^3.6.0",
    "watch": "^0.14.0",
    "watchify": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/plangular.git"
  },
  "author": "Jxnblk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jxnblk/plangular/issues"
  },
  "homepage": "https://github.com/jxnblk/plangular"
}
